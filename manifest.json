{
  "name": "jQuery DOM",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "css": [ "extension.css" ],
    "js": [ "jquery-2.1.3.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}