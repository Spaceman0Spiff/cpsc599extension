{
  "name": "Real-Time YouTube Comments",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Displays timestamped comments at the indicated time",
  "browser_action": {
    "name": "Real-Time YouTube Comments"
  },
  "content_scripts": [ {
    "css": [ "extension.css" ],
    "js": [ "jquery-2.1.3.js", "background.js" ],
    "matches": [ "https://www.youtube.com/watch?v=hhoQqN9oUpo*", "http://www.youtube.com/watch?v=hhoQqN9oUpo*"]
  }],
    "web_accessible_resources": ["frequency.png"]
}